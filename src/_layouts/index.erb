---
layout: default
---
<h1>
  <code><%= site.metadata["title"] %></code>
</h1>

<% site.collections.posts.resources.reverse.each do |post| %>
<h2>
  <a href="<%= post.relative_url %>"><%= post.data.title %></a>
</h2>
<%= post.content %>
<p><small><%= post.date.strftime("%d %b %Y") %></small></p>
<% end %>
